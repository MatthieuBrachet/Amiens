clc; clear all; close all;

global cavite Re

%% *** options ************************************************************
% si cavite = 1 : cavité raide,
%           = 2 : cavite regularisée.
cavite = 2;

%% *** données ************************************************************

N=15;
dt=0.01;
Tmax=3;
Re=100;


%% *** calculs ************************************************************
tau1=1;
[ ~, ~, ~, Ei_pp1, ~, ~, Ei_ww1 ] = compNSE(N,dt,Tmax, tau1,Re);

tau2=5;
[ ~, ~, ~, Ei_pp2, ~, ~, Ei_ww2 ] = compNSE(N,dt,Tmax, tau2,Re);

tau3=25;
[ T, ~, ~, Ei_pp3, ~, ~, Ei_ww3 ] = compNSE(N,dt,Tmax, tau3,Re);

%% graphes
figure(1)
subplot(T,